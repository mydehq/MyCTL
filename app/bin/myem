#!/bin/env bash
#  __  __       _____ __  __
# |  \/  |_   _| ____|  \/  |
# | |\/| | | | |  _| | |\/| |
# | |  | | |_| | |___| |  | |
# |_|  |_|\__, |_____|_|  |_|
#        |___/
#
# MyCTL Extension Manager (wrapper for MyCTL)

if command -v myctl &> /dev/null; then
    myctl ext "$@"
else
    echo "Error: myctl is not installed." >&2
    exit 1
fi
